<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Contrato.
    -->
    <changeSet id="20180912145702-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="locadora_id"
                                 baseTableName="contrato"
                                 constraintName="fk_contrato_locadora_id"
                                 referencedColumnNames="id"
                                 referencedTableName="locadora"/>

        <addForeignKeyConstraint baseColumnNames="seguradora_id"
                                 baseTableName="contrato"
                                 constraintName="fk_contrato_seguradora_id"
                                 referencedColumnNames="id"
                                 referencedTableName="seguradora"/>

    </changeSet>
</databaseChangeLog>
